{
  "manifest_version": 2,
  "name": "YouTube Link Modifier",
  "version": "1.0",
  "description": "Modifies YouTube links before the page is opened.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://www.youtube.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{e7bd76c2-1d6b-4e8f-9d87-88861a2edc55}"
    }
  }
}
